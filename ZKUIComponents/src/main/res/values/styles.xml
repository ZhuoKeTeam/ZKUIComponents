<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppThemeNoActionBar" parent="Theme.AppCompat.Light.NoActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="AppThemeNoActionBar"/>
    <style name="AppTheme" parent="AppThemeNoActionBar"/>


    <style name="AppThemeFull" parent="AppThemeNoActionBar">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="cons_height_mine">
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="image_left_mine">
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <style name="text_left_mine">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#333</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="image_right_mine">
        <item name="android:layout_marginRight">18dp</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#333</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>
</resources>